{
  "name": "nazan-admin-reports-frontend",
  "version": "2.1.1",
  "scripts": {
    "ng": "ng",
    "postinstall": "",
    "start": "node git-version.js; ng serve --live-reload=false",
    "start:dev": "reset && ng serve --live-reload=true",
    "start:dev2": "reset && ng serve --live-reload=false",
    "start:git": "reset && node git-version.js && ng serve --live-reload=true",
    "start:db": "json-server --watch src/shared/data/db.json",
    "build": "ng build --output-path=dist",
    "build:prod": "ng build --output-path=dist",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "git-flow": "git flow init",
    "kill:port": "kill -9 $(lsof -t -i:4200)",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "post-build": "node ./build/post-build.js",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "git:fix": "git rm -r --cached .; git add .; git commit -m 'Untracked files issue resolved to fix .gitignore'; ",
    "generate-icons": "svg-generator",
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
    "start:mt": "reset && node git-version.js && ng s --host=test.serrot.test --port=4100 --proxy-config proxy.conf.json --disable-host-check"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.6",
    "@angular/common": "^17.3.6",
    "@angular/compiler": "^17.3.6",
    "@angular/core": "^17.3.6",
    "@angular/forms": "^17.3.6",
    "@angular/platform-browser": "^17.3.6",
    "@angular/platform-browser-dynamic": "^17.3.6",
    "@angular/router": "^17.3.6",
    "@rxweb/reactive-form-validators": "^13.0.1",
    "@types/luxon": "^3.3.0",
    "chart.js": "^4.4.0",
    "csv-file-validator": "^2.1.0",
    "fs-extra": "^11.1.1",
    "git-describe": "^4.1.1",
    "luxon": "^3.3.0",
    "luxon-angular": "^6.0.0",
    "ngx-spinner": "^16.0.2",
    "ngx-toastr": "^18.0.0",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "^17.15.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.6",
    "@angular/cli": "~17.3.6",
    "@angular/compiler-cli": "^17.3.6",
    "@types/jasmine": "~4.3.0",
    "@types/papaparse": "^5.3.14",
    "autoprefixer": "^10.4.19",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.5"
  }
}
